# local is ~/.ssh/config
Host *
    PreferredAuthentications publickey

Host github22
    User git
    HostName github.com
    Port 22
    IdentityFile ~/.ssh/github.pri.key

Host github-ssh-443
    User git
    HostName ssh.github.com
    Port 443
    IdentityFile ~/.ssh/github.pri.key
    TCPKeepAlive yes
    IdentitiesOnly yes
    TCPKeepAlive yes
    # ProxyCommand connect socks5://127.0.0.1:1080 %h %p

# if it is that, the url can be `ssh://github22/${username}/${pro-name}.git`